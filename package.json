{
  "name": "whatidid",
  "version": "1.0.0",
  "description": "CLI tool to generate engineering impact reports from GitHub activity",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "whatidid": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun run build:compile",
    "build:compile": "bun build src/index.ts --outdir dist --target node --minify",
    "build:binary": "bun build src/index.ts --compile --outfile dist/whatidid",
    "build:all": "bun run build:compile && bun run build:binary",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run typecheck && bun run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "github",
    "impact",
    "report",
    "cli",
    "engineering",
    "promo",
    "self-review"
  ],
  "author": "Anup Giri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anup30giri/whatidid.git"
  },
  "bugs": {
    "url": "https://github.com/Anup30giri/whatidid/issues"
  },
  "homepage": "https://github.com/Anup30giri/whatidid#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
