{
  "name": "whatidid",
  "version": "1.0.0",
  "description": "CLI tool to generate engineering impact reports from GitHub activity",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "whatidid": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "github",
    "impact",
    "report",
    "cli",
    "engineering"
  ],
  "author": "Anup Giri",
  "license": "MIT"
}